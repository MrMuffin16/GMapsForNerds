@page
@model WebServer.Pages.BikeRacks.IndexModel

@{
    ViewData["Title"] = "Built-In DB";
}

<h1>Database -  Bike Racks (Built-In)</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BikeRack[0].Suberb)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BikeRack[0].Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BikeRack[0].Latitude)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BikeRack[0].Logitude)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.BikeRack) {
        <tr>
            <td>
                    @Html.DisplayFor(modelItem => item.Suberb)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Latitude)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Logitude)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
